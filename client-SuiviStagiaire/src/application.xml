<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

<!-- Acceuil -->
	<package name="acceuil" namespace="/accueil" extends="struts-default">
		<action name="accueil">
			<result>/index.jsp</result>
		</action>
	</package>
	
<!-- auto-evaluation -->
	<package name="auto-evalution" namespace="/auto-evalution"  extends="struts-default">
		<action name="">
			<result></result>
		</action>
	
	</package>
	
<!-- boostrap -->	
	<package name="boostrap" namespace="" extends="struts-default">
		
		<action name="bootstrap-css">
			<result>/WEB-INF/bootstrap-3.3.7-dist/css/bootstrap.min.css</result>
		</action>
		
		<action name="bootstrap-theme">
			<result>/WEB-INF/bootstrap-3.3.7-dist/css/bootstrap-theme.min.css</result>
		</action>
		
		<action name="bootstrap-js">
			<result>"WebContent/WEB-INF/bootstrap-3.3.7-dist/js/bootstrap.min.js"</result>
		</action>
		
	</package>
	

	
</struts>